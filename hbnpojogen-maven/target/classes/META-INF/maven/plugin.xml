<?xml version="1.0" encoding="UTF-8"?>
<plugin>
  <name>Hibernate Synchronizer Maven Plugin</name>
  <description>Hibernate Synchronizer Maven Plugin</description>
  <groupId>com.felees</groupId>
  <artifactId>hbnpojogen-maven-plugin</artifactId>
  <version>1.4.32</version>
  <goalPrefix>hbnpojogen</goalPrefix>
  <isolatedRealm>false</isolatedRealm>
  <inheritedByDefault>true</inheritedByDefault>
  <mojos>
    <mojo>
      <goal>generate</goal>
      <description>The Hibernate Sychronizer mojo.</description>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>true</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
      <inheritedByDefault>true</inheritedByDefault>
      <phase>generate-sources</phase>
      <implementation>com.felees.hbnpojogen.SynchronizerMojo</implementation>
      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <threadSafe>false</threadSafe>
      <parameters>
        <parameter>
          <name>configFile</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>The name of the XML config file to use. If you omit this, attempt to load hbnpojogen.config.xml
from the classpath.</description>
        </parameter>
        <parameter>
          <name>generateModel</name>
          <type>java.lang.Boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>If this is declared, the model generation will start.</description>
        </parameter>
        <parameter>
          <name>ip</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>ipaddress to use as source (override).</description>
        </parameter>
        <parameter>
          <name>mavenProject</name>
          <type>org.apache.maven.project.MavenProject</type>
          <required>true</required>
          <editable>false</editable>
          <description>The project object model.</description>
        </parameter>
        <parameter>
          <name>modelArtifactId</name>
          <type>java.lang.String</type>
          <required>true</required>
          <editable>true</editable>
          <description>The artifactId of the child that will contain the generated sources.</description>
        </parameter>
      </parameters>
      <configuration>
        <configFile implementation="java.lang.String">${configFile}</configFile>
        <generateModel implementation="java.lang.Boolean">${generateModel}</generateModel>
        <ip implementation="java.lang.String">${ip}</ip>
        <mavenProject implementation="org.apache.maven.project.MavenProject">${project}</mavenProject>
        <modelArtifactId implementation="java.lang.String">${modelArtifactId}</modelArtifactId>
      </configuration>
    </mojo>
  </mojos>
  <dependencies>
    <dependency>
      <groupId>com.felees</groupId>
      <artifactId>hbnpojogen-core</artifactId>
      <type>jar</type>
      <version>1.4.32</version>
    </dependency>
